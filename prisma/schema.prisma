datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id       Int      @id @default(autoincrement())
    nama     String
    email    String   @unique
    password String
    createAt DateTime @default(now())
    updateAt DateTime @updatedAt
}

model Book {
    id       Int      @id @default(autoincrement())
    judul    String
    stok     Int
    harga    Int
    createAt DateTime @default(now())
    updateAt DateTime @updatedAt
}
